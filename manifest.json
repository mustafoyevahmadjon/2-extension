{
  "manifest_version": 3,
  "name": "Timero Extension",
  "version": "1.0.0",
  "description": "Helps your focus and get things done !",
  "icons": {
    "16": "clock.png",
    "32": "clock.png",
    "48": "clock.png",
    "128": "clock.png"
  },
  "action": {
    "default_icon": {
      "16": "clock.png",
      "32": "clock.png",
      "48": "clock.png",
      "128": "clock.png"
    },
    "default_title": "Timer Extension Action",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage","alarms","notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options/options.html"
}
